<%# nav-tabsからの分岐出力制御 %>
<% case @transition_value %>
<% when "will" %>
  $('#wcm-container').html("<%= j(render partial: "users/wcm_seat/modal/work_space", locals: { instance: @transition_value }) %>");
<% when "can" %>
  $('#wcm-container').html("<%= j(render partial: "users/wcm_seat/modal/work_space", locals: { instance: @transition_value }) %>");
<% when "must" %>
  $('#wcm-container').html("<%= j(render partial: "users/wcm_seat/modal/work_space", locals: { instance: @transition_value }) %>");
<% else %>
  <% render template: "user/show" %>
<% end %>